<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>com.example.HelloServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/h1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StartDispatch1</servlet-name>
        <servlet-class>com.example.StartDispatch1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StartDispatch1</servlet-name>
        <url-pattern>/StartDispatch1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StartDispatch2</servlet-name>
        <servlet-class>com.example.StartDispatch2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StartDispatch2</servlet-name>
        <url-pattern>/StartDispatch2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StartRedirect1</servlet-name>
        <servlet-class>com.example.StartRedirect1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StartRedirect1</servlet-name>
        <url-pattern>/StartRedirect1</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>FinishRedirect</servlet-name>
        <servlet-class>com.example.FinishRedirect</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FinishRedirect</servlet-name>
        <url-pattern>/FinishRedirect1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Servlet1</servlet-name>
        <servlet-class>com.example.Servlet1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet1</servlet-name>
        <url-pattern>/s1</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Servlet2</servlet-name>
        <servlet-class>com.example.Servlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet2</servlet-name>
        <url-pattern>/s2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>OutJpg</servlet-name>
        <servlet-class>com.example.OutJpg</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OutJpg</servlet-name>
        <url-pattern>/OutJpg</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginProcess</servlet-name>
        <servlet-class>com.example.action.LoginProcessAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginProcess</servlet-name>
        <url-pattern>/loginProcess.do</url-pattern>
    </servlet-mapping>



</web-app>
